{
  "name": "solid-currency-input-field",
  "version": "0.1",
  "description": "Solid-JS <input/> component for formatting currency and numbers.",
  "license": "MIT",
  "author": "nsaritzky",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsaritzky/solid-currency-input-field.git"
  },
  "homepage": "https://github.com/nsaritzky/solid-currency-input-field#readme",
  "bugs": {
    "url": "https://github.com/nsaritzky/solid-currency-input-field/issues"
  },
  "files": ["dist"],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index/index.js",
  "module": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "browser": {},
  "exports": {
    ".": {
      "solid": "./dist/index/index.jsx",
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      }
    },
    "./examples": {
      "solid": {
        "development": "./dist/examples/dev.jsx",
        "import": "./dist/examples/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/examples/index.d.ts",
          "default": "./dist/examples/dev.js"
        }
      },
      "import": {
        "types": "./dist/examples/index.d.ts",
        "default": "./dist/examples/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "examples": ["./dist/examples/index.d.ts"]
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{ts,tsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.15",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-solid": "^0.12.1",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "solid-js": "^1.7.8",
    "tsup": "^7.1.0",
    "tsup-preset-solid": "^2.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.33.0"
  },
  "keywords": ["solid", "front-end", "form", "input", "field"],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  }
}
